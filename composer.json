{
    "name": "mileschou/laradra",
    "description": "Laravel with Hydra ",
    "keywords": ["laravel", "hydra"],
    "type": "library",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/MilesChou/laradra/issues",
        "source": "https://github.com/MilesChou/laradra"
    },
    "authors": [
        {
            "name": "Miles Chou",
            "email": "jangconan@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3 | ^8.0",
        "illuminate/database": "^8 | ^9",
        "illuminate/events": "^8"
    },
    "require-dev": {
        "illuminate/log": "^8 | ^9",
        "laravel-zero/framework": "^8 | ^9",
        "mockery/mockery": "^1.4.3",
        "pestphp/pest": "^1.3",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "Laradra\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "app/",
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
